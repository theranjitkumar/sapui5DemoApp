<mvc:View controllerName="sap.ui.demo.basicTemplate.controller.About" displayBlock="true" 
    xmlns="sap.m" 
    xmlns:mvc="sap.ui.core.mvc">
    <Page id="idEmployee" title="Employee List">
        <content>
            <List noDataText="Drop list items here" id="list0" items="{appData>/employee}" headerText="Employee Details">
                <items>
                    <StandardListItem type="Navigation" title="{appData>empName}" description="Id is: {appData>empId}" icon="sap-icon://picture" id="item0"/>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>