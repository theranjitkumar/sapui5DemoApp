<mvc:View controllerName="sap.ui.demo.basicTemplate.controller.About" displayBlock="true" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="idAbout" title="{i18n>title}">
		<content>
			<!-- <Label text="{/users}"></Label> -->
			<List noDataText="Drop list items here" id="idlist" items="{/users}" headerText="Users List">
				<items>
					<StandardListItem type="Navigation" title="{name}" description="Name is: {email}" icon="sap-icon://picture" id="item0"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>